@{
}
@model Task1.Models.Student

@{
    ViewData["Title"] = "StudentView";
}
@using (Html.BeginForm("Create", "Student", FormMethod.Post))
{
    @Html.LabelFor(m => m.FirstName)
    @Html.TextBoxFor(m => m.FirstName)
    <br />
    @Html.LabelFor(m => m.LastName)
    @Html.TextBoxFor(m => m.LastName)
    <br />
    @Html.LabelFor(m => m.BranchName)
    @Html.DropDownListFor(m => m.BranchName, ViewBag.BranchNames as SelectList, "Select Branch")
    <br />
    @Html.LabelFor(m => m.YearName)
    @Html.TextBoxFor(m => m.YearName)
    <br />
    <input type="submit" value="Save" />
}

